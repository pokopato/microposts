<h1>アカウント情報の変更</h1>
<div class="col-md-6 col-md-offset-3">
  <%= form_for(@user) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :email %>
    <%= f.text_field :email %>
  
    <%= f.submit '更新！', class: "btn btn-lg btn-primary" %>
  <% end %>
</div>